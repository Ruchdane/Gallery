{
    "name": "galery",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "tauri dev",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "watch": "npx onchange '**/*' -- npx prettier --write --ignore-unknown {{changed}}",
        "format": "prettier '**/*.{js,jsx}' --write",
        "lint": "eslint '**/*.{js,jsx}' --fix",
        "test": "npm-run-all test:*",
        "test:unit": "npx vitest -r src-mithril/component",
        "test:view": "npx vitest -r src-mithril/view"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.3.0",
        "@tauri-apps/cli": "^1.0.0",
        "@vitest/ui": "^0.18.0",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.4",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.30.1",
        "jsdom": "^20.0.0",
        "mithril-query": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "2.7.1",
        "sass": "^1.53.0",
        "vite": "^2.9.9",
        "vite-plugin-eslint": "^1.6.1",
        "vitest": "^0.17.0"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.5",
        "@tauri-apps/api": "^1.0.1",
        "bootstrap-icons": "^1.8.3",
        "construct-ui": "^0.3.3",
        "mithril": "^2.2.2",
        "ospec": "^4.1.6"
    }
}
